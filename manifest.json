{
  "name": "audio history",
  "version": "1.0",
  "description": "keeps a history of all audio played in chrome",
  "background": { 
    "scripts": ["background.js"],
    "persistent": true 
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "declarativeWebRequest",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "manifest_version": 2
}
