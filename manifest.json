{
  "name": "audio history",
  "version": "1.0",
  "description": "keeps a history of all audio played in chrome",
  "background": { 
    "scripts": ["background.js"],
    "persistent": true 
  },
  "page_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "declarativeWebRequest",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
